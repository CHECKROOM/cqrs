{
  "name": "@nestjs/cqrs",
  "version": "6.0.0",
  "description": "A lightweight CQRS module for Nest framework (node.js)",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.json",
    "installandbuild": "npm install && npm run build",
    "precommit": "lint-staged",
    "prettier": "prettier src/**/*.ts --write && git status",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public"
  },
  "devDependencies": {
    "@nestjs/common": "6.2.0",
    "@types/node": "11.13.0",
    "@types/jest": "24.0.11",
    "husky": "1.3.1",
    "jest": "24.7.0",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "rxjs": "6.4.0",
    "ts-jest": "24.0.1",
    "typescript": "3.4.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.2.0",
    "@nestjs/core": "^6.2.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "^6.4.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
